                  "type": "integer",
                  "minimum": 1
                },
                "totalThoughts": {
                  "minimum": 1,
                  "maximum": 9007199254740991,
                  "description": "Estimated total thoughts needed (numeric value, e.g., 5, 10)",
                  "type": "integer"
                },
                "revisesThought": {
                  "minimum": 1,
                  "maximum": 9007199254740991,
                  "description": "Which thought is being reconsidered",
                  "type": "integer"
